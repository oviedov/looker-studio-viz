{
  "data": [{
    "id": "progress",
    "label": "Progress Data",
    "elements": [
      {
        "id": "labels",
        "label": "Label Dimension",
        "type": "DIMENSION",
        "options": {
          "min": 1,
          "max": 1,
          "supportedTypes": ["DEFAULT"]
        }
      },
      {
        "id": "values",
        "label": "Value Metrics",
        "type": "METRIC",
        "options": {
          "min": 1,
          "max": 10
        }
      },
      {
        "id": "maxResults",
        "label": "Max Results",
        "type": "MAX_RESULTS",
        "options": {
          "max": 25
        }
      }
    ]
  }],
  "style": [
    {
      "id": "appearance",
      "label": "Appearance",
      "elements": [
        {
          "id": "barColor",
          "label": "Progress Bar Color",
          "defaultValue": {
            "color": "#4285F4"
          },
          "type": "FILL_COLOR"
        },
        {
          "id": "barHeight",
          "label": "Bar Height (px)",
          "defaultValue": 24,
          "type": "INTERVAL",
          "options": {
            "min": 10,
            "max": 50
          }
        },
        {
          "id": "barSpacing",
          "label": "Spacing Between Bars (px)",
          "defaultValue": 16,
          "type": "INTERVAL",
          "options": {
            "min": 5,
            "max": 50
          }
        },
        {
          "id": "borderRadius",
          "label": "Bar Corner Radius",
          "defaultValue": 4,
          "type": "BORDER_RADIUS"
        },
        {
          "id": "showValues",
          "label": "Show Percentage Values",
          "defaultValue": true,
          "type": "CHECKBOX"
        }
      ]
    },
    {
      "id": "text",
      "label": "Text Format",
      "elements": [
        {
          "id": "labelFontSize",
          "label": "Label Font Size",
          "defaultValue": 14,
          "type": "FONT_SIZE"
        },
        {
          "id": "valueFontSize",
          "label": "Value Font Size",
          "defaultValue": 14,
          "type": "FONT_SIZE"
        },
        {
          "id": "labelFontColor",
          "label": "Label Font Color",
          "defaultValue": {
            "color": "#000000"
          },
          "type": "FONT_COLOR"
        },
        {
          "id": "valueFontColor",
          "label": "Value Font Color",
          "defaultValue": {
            "color": "#000000"
          },
          "type": "FONT_COLOR"
        },
        {
          "id": "fontFamily",
          "label": "Font Family",
          "defaultValue": "Roboto",
          "type": "FONT_FAMILY"
        }
      ]
    }
  ],
  "interactions": [
    {
      "id": "interactions",
      "supportedActions": ["FILTER"]
    }
  ]
}